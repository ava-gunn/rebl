{
  "tasks": {
    "server": "deno run --allow-net --allow-read server.ts",
    "repl": "deno run --allow-net --allow-read repl.ts --quiet",
    "replv": "deno run --allow-net --allow-read repl.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1"
  },
  "files": {
    "include": ["server.ts", "repl.ts"],
    "compilerOptions": {
      "lib": ["deno.ns"]
    }
  },
  "exclude": ["public/"],
  "compilerOptions": {
    "lib": ["deno.window"]
  }
}
